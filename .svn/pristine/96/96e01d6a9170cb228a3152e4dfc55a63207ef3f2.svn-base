/*IN CASE OF FORMAT ERROS, COMMENT OUT THE FOLLOWING: */
.headerUL {
    list-style: none;
    padding: 0px;
    margin: 0px;
    display: inline;
}

.headerUL .headerIL {
    font-family: "Ariel", Helvetica, san-serif;
    font-size: 16px;
    font-weight: bold;
    /*color:white;  */  
    line-height: 50px;
    display: block;
    float: right;
    text-decoration: none;
    position: relative;
}
.headerIL .headerUL {
    display: none;
}
.headerUL .headerIL .headerA {
    display: block;
    background: #5A0000;
    padding-right: 10px;
    padding-left: 10px;
    padding-top: 0px;
    padding-bottom: 0px;
    text-decoration: none;
    white-space: nowrap;
    color: white;
}
.headerUL .headerIL .headerA:hover {
    background: #AA0000;
}
.headerIL:hover .headerUL {
    display: block;
    position: absolute;
    cursor: pointer;
}
.headerIL:hover .headerIL {
    float: none;
    cursor: pointer;
}
.headerIL:hover .headerA {
    background: #8A0000;
    cursor: pointer;
}
