.btn {

	cursor: pointer;
	display: inline-block;
	margin: 15px 30px;
	outline: none;
	position: relative;
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
}

.btn:after {
	content: '';
	position: absolute;
	z-index: -1;
	-webkit-transition: all 0.3s;
	-moz-transition: all 0.3s;
	transition: all 0.3s;
        display: inline-block;
}
.icon-add:before {
	content: url(../images/plusSign.png); 
        vertical-align:middle;
}
.icon-remove:before {
	content: url(../images/xSign.png); 
        vertical-align:middle;
}
.icon-search:before {
	content: url(../images/searchicon.png); 
        vertical-align:middle;
}

.icon-arrow-right:before {
	content: url(../images/arrowRight.png); 
        vertical-align:middle;
}
.icon-arrow-left:before {
	content: url(../images/arrowLeft.png); 
        vertical-align:middle;
}
.icon-menu:before {
	content: url(../images/menuicon.png); 
        vertical-align:middle;
}
.icon-edit:before {
	content: url(../images/editIcon.png); 
        vertical-align:middle;
}
.icon-update:before {
	content: url(../images/update.png); 
        vertical-align:middle;
}
.icon-submit:before {
	content: url(../images/submitIcon.png); 
        vertical-align:middle;
}
.icon-save:before {
	content: url(../images/SaveIcon.png); 
        vertical-align:middle;
}
 /*Button 4*/ 
.btn-4 {
        padding-right: 35px;
        padding-top: 10px;
	color: #fff;
	overflow: hidden;
}


.btn-4:before {
	position: relative;
	height: 100%;
	font-size: 125%;
        display: inline-block;
	color: #fff;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	transition: all .3s;
}

.btn-4:active:before {
	color: #17954c;
}

 /*Button 4a*/ 
.btn-4a:before {
	left: 155%;
	top: 0;
}

.btn-4a:hover:before {
	left: 75%;
        display: inline-block;
}
