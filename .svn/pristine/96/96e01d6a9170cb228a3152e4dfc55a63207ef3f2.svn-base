button,input[type = "submit"] {
    background: #5a0000;
    background-image: -webkit-linear-gradient(top, #5a0000, #8c0e0e);
    background-image: -moz-linear-gradient(top, #5a0000, #8c0e0e);
    background-image: -ms-linear-gradient(top, #5a0000, #8c0e0e);
    background-image: -o-linear-gradient(top, #5a0000, #8c0e0e);
    background-image: linear-gradient(to bottom, #5a0000, #8c0e0e);
    -webkit-border-radius: 28;
    -moz-border-radius: 28;
    border-radius: 28px;
    font-family: Arial;
    color: #ffffff;
    padding: 5px 20px 5px 20px;
    border:2px solid #5A0000;

}

button:hover,input[type = "submit"]:hover {
    /*background: #fc3c3c;*/
    background-image: -webkit-linear-gradient(top, #fc3c3c, #5a0000);
    background-image: -moz-linear-gradient(top, #fc3c3c, #5a0000);
    background-image: -ms-linear-gradient(top, #fc3c3c, #5a0000);
    background-image: -o-linear-gradient(top, #fc3c3c, #5a0000);
    background-image: linear-gradient(to bottom, #fc3c3c, #5a0000);
}

