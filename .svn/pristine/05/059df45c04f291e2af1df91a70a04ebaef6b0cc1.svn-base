package common;

/**
 * Represents a <em>service activity</em> performed by the user. This includes 
 * community service and other miscellaneous experience and leadership.
 *
 * @author Gryphon Ayers (2019)
 */
public class SvcActivity {
    private final int svcActivityID; // private key
    private int application; // foreign key - refers to applicationID
    private String type;
    private int supportDocument; // foreign key - refers to mediaID

    public SvcActivity(int svcActivityID) {
        this.svcActivityID = svcActivityID;
    }

    public int getSvcActivityID() {
        return svcActivityID;
    }

    public int getApplication() {
        return application;
    }

    public void setApplication(int application) {
        this.application = application;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public int getSupportDocument() {
        return supportDocument;
    }

    public void setSupportDocument(int supportDocument) {
        this.supportDocument = supportDocument;
    }
}
